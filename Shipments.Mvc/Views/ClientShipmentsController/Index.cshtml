@model List<Shipments.Shared.Contracts.Shipments.Responses.ShipmentListItemDto>

<h2>My Shipments</h2>

<table border="1" cellpadding="6">
    <tr>
        <th>Id</th>
        <th>Status</th>
        <th>Recipient</th>
        <th>City</th>
        <th>Created</th>
    </tr>

    @foreach (var s in Model)
    {
        <tr>
            <td>@s.Id</td>
            <td>@s.Status</td>
            <td>@s.RecipientName</td>
            <td>@s.RecipientCity</td>
            <td>@s.CreatedAtUtc</td>
        </tr>
    }
</table>